{
  "name": "livex-backend-mvp-nest",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:dev:bun": "bun node_modules/.bin/nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "bun dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "dev": "nodemon --watch src --ext ts,js --exec \"node --enable-source-maps -r ts-node/register src/main.ts\"",
    "dev:bun": "bun --watch src/main.ts",
    "worker:dev": "nodemon --watch src --ext ts,js --exec \"node --enable-source-maps -r ts-node/register src/worker.ts\"",
    "worker": "node dist/worker.js",
    "notification-worker:dev": "nodemon --watch src --ext ts,js --exec \"node --enable-source-maps -r ts-node/register src/notification-worker.ts\"",
    "notification-worker": "node dist/notification-worker.js",
    "booking-expiry-worker:dev": "nodemon --watch src --ext ts,js --exec \"node --enable-source-maps -r ts-node/register src/booking-expiry-worker.ts\"",
    "booking-expiry-worker": "node dist/booking-expiry-worker.js",
    "payment-reconciliation-worker:dev": "nodemon --watch src --ext ts,js --exec \"node --enable-source-maps -r ts-node/register src/payment-reconciliation-worker.ts\"",
    "payment-reconciliation-worker": "node dist/payment-reconciliation-worker.js"
  },
  "dependencies": {
    "@azure/identity": "^4.12.0",
    "@azure/storage-blob": "^12.28.0",
    "@fastify/cors": "^11.1.0",
    "@fastify/helmet": "^13.0.2",
    "@fastify/multipart": "^9.3.0",
    "@fastify/static": "^8.3.0",
    "@fastify/swagger": "^9.6.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@livex/contracts": "^0.1.0",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.0",
    "@nestjs/platform-fastify": "^11.1.9",
    "@nestjs/throttler": "^6.5.0",
    "@types/nodemailer": "^7.0.2",
    "amqplib": "^0.10.9",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "firebase-admin": "^13.6.0",
    "handlebars": "^4.7.8",
    "nodemailer": "^7.0.7",
    "nodemon": "^3.1.10",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/amqplib": "^0.10.7",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
